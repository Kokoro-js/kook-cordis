{
  "name": "kook-cordis",
  "version": "1.7.4",
  "description": "Kook 在 Cordis 下的 SDK 包装。",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "agent-base": "^7.1.0",
    "axios": "^1.6.7",
    "cordis": "3.4.1",
    "cosmokit": "^1.5.2",
    "fast-fuzzy": "^1.12.0",
    "mime-db": "^1.52.0",
    "pino": "^8.19.0",
    "schemastery": "^3.14.3",
    "type-flag": "^3.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.32.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/tap": "^15.0.11",
    "cross-env": "^7.0.3",
    "pino-pretty": "^10.3.1",
    "prettier": "3.1.1",
    "tap": "^18.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "prepublish": "tsc",
    "dev": "if \"%OS%\"==\"Windows_NT\" (chcp 65001) && npm run dev:start",
    "dev:start": "cross-env LEVEL=trace ts-node-dev --respawn --debounce 1000 src/app.ts | pino-pretty"
  },
  "files": [
    "dist/",
    "!/dist/app.*"
  ],
  "keywords": [],
  "author": "",
  "license": "LGPL-3.0"
}
