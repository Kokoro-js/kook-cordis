{
  "name": "kook-codis",
  "version": "1.0.0",
  "description": "Kook 在 Cordis 下的 SDK 包装。",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@satorijs/satori": "3.0.0-rc.0",
    "axios": "^1.5.1",
    "cordis": "^2.8.8",
    "cosmokit": "^1.4.5",
    "minizlib": "^2.1.2",
    "pino": "^8.15.3",
    "pino-debug": "^2.0.0",
    "schemastery": "^3.13.1",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.32.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@types/tap": "^15.0.5",
    "@types/yargs": "^17.0.28",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "pino-pretty": "^10.2.3",
    "prettier": "3.0.3",
    "tap": "^16.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "cross-env LEVEL=debug DEBUG=webhook ts-node-dev --respawn --debounce 1000 src/app.ts | pino-pretty"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
