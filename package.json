{
  "name": "kook-cordis",
  "version": "1.0.2",
  "description": "Kook 在 Cordis 下的 SDK 包装。",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "cordis": "^2.9.3",
    "cosmokit": "^1.5.1",
    "fast-fuzzy": "^1.12.0",
    "pino": "^8.16.0",
    "schemastery": "^3.14.0",
    "type-flag": "^3.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.32.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/tap": "^15.0.10",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "pino-debug": "^2.0.0",
    "pino-pretty": "^10.2.3",
    "prettier": "3.0.3",
    "tap": "^18.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "cross-env LEVEL=debug DEBUG=webhook ts-node-dev --respawn --debounce 1000 src/app.ts | pino-pretty"
  },
  "files": [
    "dist/",
    "index.js",
    "index.d.ts"
  ],
  "keywords": [],
  "author": "",
  "license": "LGPL-3.0"
}
