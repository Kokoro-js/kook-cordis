{
  "name": "kook-cordis",
  "version": "1.10.16",
  "description": "Kook 在 Cordis 下的 SDK 包装。",
  "repository": "github:Kokoro-js/kook-cordis",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "bun": "./src/index.ts",
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "agent-base": "^7.1.1",
    "axios": "^1.7.2",
    "cordis": "3.4.1",
    "cosmokit": "1.5.2",
    "fast-fuzzy": "^1.12.0",
    "kafkajs": "^2.2.4",
    "mime-db": "^1.53.0",
    "msgpackr": "^1.11.0",
    "pino": "^8.21.0",
    "schemastery": "^3.14.6",
    "type-flag": "^3.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.32.0",
    "ws": "^8.18.0"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "build": "pkgroll",
    "prepublish": "pkgroll",
    "dev": "if \"%OS%\"==\"Windows_NT\" (chcp 65001) && npm run dev:start",
    "dev:start": "cross-env LEVEL=trace ts-node-dev --respawn --debounce 1000 ../../src/app.ts | pino-pretty"
  },
  "files": [
    "dist/",
    "!/dist/app.*"
  ],
  "keywords": [],
  "author": "",
  "license": "LGPL-3.0",
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "@types/node": "^20.14.13",
    "pkgroll": "^2.4.2",
    "typescript": "^5.5.4"
  }
}
