{
  "name": "kook-cordis",
  "version": "1.11.4",
  "type": "module",
  "description": "Kook 在 Cordis 下的 SDK 包装。",
  "repository": "github:Kokoro-js/kook-cordis",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "bun": "./src/index.ts",
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "agent-base": "^7.1.3",
    "axios": "^1.7.9",
    "cordis": "3.4.1",
    "cosmokit": "1.7.2",
    "fast-fuzzy": "^1.12.0",
    "mime-db": "^1.53.0",
    "pino": "^9.6.0",
    "schemastery": "^3.16.0",
    "type-flag": "^3.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.51.0",
    "ws": "^8.18.0",
    "@types/ws": "^8.5.14"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "build": "pkgroll --minify",
    "prepublish": "npm run build",
    "dev": "if \"%OS%\"==\"Windows_NT\" (chcp 65001) && npm run dev:start",
    "dev:start": "cross-env LEVEL=trace ts-node-dev --respawn --debounce 1000 ../../src/app.ts | pino-pretty"
  },
  "files": [
    "dist/",
    "!/dist/app.*"
  ],
  "keywords": [],
  "author": "",
  "license": "LGPL-3.0",
  "peerDependencies": {
    "@types/node": "*",
    "pkgroll": "*",
    "typescript": "*"
  }
}
